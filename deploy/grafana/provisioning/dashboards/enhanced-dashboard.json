{
  "id": null,
  "uid": "nextcloud-perf-monitor-enhanced",
  "title": "Nextcloud Performance Monitor - Enhanced",
  "tags": ["nextcloud", "performance", "monitoring"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 3,
  "refresh": "30s",
  "panels": [
    {
      "type": "row",
      "title": "Performance Overview",
      "collapsed": false,
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
      "id": 100
    },
    {
      "type": "stat",
      "title": "Upload Duration (s)",
      "targets": [
        {
          "expr": "avg_over_time(nextcloud_test_duration_seconds{service=~\"$service\",instance=~\"$instance\",type=\"upload\"}[5m])",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "s",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 300 },
              { "color": "red", "value": 600 }
            ]
          }
        } 
      },
      "gridPos": { "x": 0, "y": 1, "w": 4, "h": 4 },
      "id": 1
    },
    {
      "type": "stat",
      "title": "Download Duration (s)",
      "targets": [
        {
          "expr": "avg_over_time(nextcloud_test_duration_seconds{service=~\"$service\",instance=~\"$instance\",type=\"download\"}[5m])",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "s",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 60 },
              { "color": "red", "value": 120 }
            ]
          }
        } 
      },
      "gridPos": { "x": 4, "y": 1, "w": 4, "h": 4 },
      "id": 2
    },
    {
      "type": "stat",
      "title": "Upload Speed (MB/s)",
      "targets": [
        {
          "expr": "avg_over_time(nextcloud_test_speed_mbytes_per_sec{service=~\"$service\",instance=~\"$instance\",type=\"upload\"}[5m])",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "MBs",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "green", "value": 20 }
            ]
          }
        } 
      },
      "gridPos": { "x": 8, "y": 1, "w": 4, "h": 4 },
      "id": 3
    },
    {
      "type": "stat",
      "title": "Download Speed (MB/s)",
      "targets": [
        {
          "expr": "avg_over_time(nextcloud_test_speed_mbytes_per_sec{service=~\"$service\",instance=~\"$instance\",type=\"download\"}[5m])",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "MBs",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "green", "value": 50 }
            ]
          }
        } 
      },
      "gridPos": { "x": 12, "y": 1, "w": 4, "h": 4 },
      "id": 4
    },
    {
      "type": "stat",
      "title": "Success Rate (%)",
      "targets": [
        {
          "expr": "avg_over_time(nextcloud_test_success{service=~\"$service\",instance=~\"$instance\"}[5m]) * 100",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "percent",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 95 },
              { "color": "green", "value": 99 }
            ]
          }
        } 
      },
      "gridPos": { "x": 16, "y": 1, "w": 4, "h": 4 },
      "id": 5
    },
    {
      "type": "stat",
      "title": "Active Tests",
      "targets": [
        {
          "expr": "count(up{job=\"monitor-agent\"})",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        } 
      },
      "gridPos": { "x": 20, "y": 1, "w": 4, "h": 4 },
      "id": 6
    },
    {
      "type": "row",
      "title": "Error Analysis & Reliability",
      "collapsed": false,
      "gridPos": { "x": 0, "y": 5, "w": 24, "h": 1 },
      "id": 200
    },
    {
      "type": "timeseries",
      "title": "Error Rate Trends Over Time",
      "targets": [
        {
          "expr": "rate(nextcloud_test_errors_total{service=~\"$service\",instance=~\"$instance\"}[5m]) * 100",
          "legendFormat": "{{service}} - {{instance}} - {{type}}",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "percent",
          "min": 0,
          "color": { "mode": "palette-classic" }
        } 
      },
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 6 },
      "id": 7,
      "options": {
        "tooltip": { "mode": "multi", "sort": "none" },
        "legend": { "displayMode": "table", "placement": "bottom" }
      }
    },
    {
      "type": "timeseries",
      "title": "Success Rate Trends",
      "targets": [
        {
          "expr": "avg_over_time(nextcloud_test_success{service=~\"$service\",instance=~\"$instance\"}[5m]) * 100",
          "legendFormat": "{{service}} - {{instance}} - {{type}}",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": { "mode": "palette-classic" }
        } 
      },
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 6 },
      "id": 8,
      "options": {
        "tooltip": { "mode": "multi", "sort": "none" },
        "legend": { "displayMode": "table", "placement": "bottom" }
      }
    },
    {
      "type": "row",
      "title": "Chunk Upload Statistics & Network Analysis",
      "collapsed": false,
      "gridPos": { "x": 0, "y": 12, "w": 24, "h": 1 },
      "id": 300
    },
    {
      "type": "stat",
      "title": "Total Chunks Uploaded (24h)",
      "targets": [
        {
          "expr": "sum(increase(nextcloud_chunks_uploaded_total{service=~\"$service\",instance=~\"$instance\"}[24h]))",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1000 },
              { "color": "red", "value": 5000 }
            ]
          }
        } 
      },
      "gridPos": { "x": 0, "y": 13, "w": 4, "h": 4 },
      "id": 9
    },
    {
      "type": "stat",
      "title": "Chunk Retry Rate (%)",
      "targets": [
        {
          "expr": "(sum(increase(nextcloud_chunk_retries_total{service=~\"$service\",instance=~\"$instance\"}[1h])) / sum(increase(nextcloud_chunks_uploaded_total{service=~\"$service\",instance=~\"$instance\"}[1h]))) * 100",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "percent",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 15 }
            ]
          }
        } 
      },
      "gridPos": { "x": 4, "y": 13, "w": 4, "h": 4 },
      "id": 10
    },
    {
      "type": "stat",
      "title": "Avg Chunk Upload Time (s)",
      "targets": [
        {
          "expr": "avg_over_time(nextcloud_chunk_upload_duration_seconds{service=~\"$service\",instance=~\"$instance\"}[1h])",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "s",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 10 }
            ]
          }
        } 
      },
      "gridPos": { "x": 8, "y": 13, "w": 4, "h": 4 },
      "id": 11
    },
    {
      "type": "stat",
      "title": "Network Latency (ms)",
      "targets": [
        {
          "expr": "avg_over_time(nextcloud_network_latency_ms{service=~\"$service\",instance=~\"$instance\"}[5m])",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "ms",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 500 }
            ]
          }
        } 
      },
      "gridPos": { "x": 12, "y": 13, "w": 4, "h": 4 },
      "id": 12
    },
    {
      "type": "stat",
      "title": "Connection Timeouts (24h)",
      "targets": [
        {
          "expr": "sum(increase(nextcloud_connection_timeouts_total{service=~\"$service\",instance=~\"$instance\"}[24h]))",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 20 }
            ]
          }
        } 
      },
      "gridPos": { "x": 16, "y": 13, "w": 4, "h": 4 },
      "id": 13
    },
    {
      "type": "stat",
      "title": "Circuit Breaker Status",
      "targets": [
        {
          "expr": "nextcloud_circuit_breaker_state{service=~\"$service\",instance=~\"$instance\"}",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "short",
          "mappings": [
            { "options": { "0": { "text": "CLOSED", "color": "green" } } },
            { "options": { "1": { "text": "OPEN", "color": "red" } } },
            { "options": { "2": { "text": "HALF-OPEN", "color": "yellow" } } }
          ],
          "color": { "mode": "thresholds" }
        } 
      },
      "gridPos": { "x": 20, "y": 13, "w": 4, "h": 4 },
      "id": 14
    },
    {
      "type": "timeseries",
      "title": "Chunk Upload Performance Over Time",
      "targets": [
        {
          "expr": "rate(nextcloud_chunks_uploaded_total{service=~\"$service\",instance=~\"$instance\"}[5m])",
          "legendFormat": "{{service}} - {{instance}} - Chunks/sec",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        },
        {
          "expr": "rate(nextcloud_chunk_retries_total{service=~\"$service\",instance=~\"$instance\"}[5m])",
          "legendFormat": "{{service}} - {{instance}} - Retries/sec",
          "refId": "B",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "ops",
          "color": { "mode": "palette-classic" }
        } 
      },
      "gridPos": { "x": 0, "y": 17, "w": 12, "h": 6 },
      "id": 15,
      "options": {
        "tooltip": { "mode": "multi", "sort": "none" },
        "legend": { "displayMode": "table", "placement": "bottom" }
      }
    },
    {
      "type": "timeseries",
      "title": "Network Latency Per Instance",
      "targets": [
        {
          "expr": "avg_over_time(nextcloud_network_latency_ms{service=~\"$service\",instance=~\"$instance\"}[1m])",
          "legendFormat": "{{service}} - {{instance}}",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "ms",
          "color": { "mode": "palette-classic" }
        } 
      },
      "gridPos": { "x": 12, "y": 17, "w": 12, "h": 6 },
      "id": 16,
      "options": {
        "tooltip": { "mode": "multi", "sort": "none" },
        "legend": { "displayMode": "table", "placement": "bottom" }
      }
    },
    {
      "type": "row",
      "title": "Performance Trends & Analysis",
      "collapsed": false,
      "gridPos": { "x": 0, "y": 23, "w": 24, "h": 1 },
      "id": 400
    },
    {
      "type": "timeseries",
      "title": "Upload/Download Speed Comparison Over Time",
      "targets": [
        {
          "expr": "avg_over_time(nextcloud_test_speed_mbytes_per_sec{service=~\"$service\",instance=~\"$instance\",type=\"upload\"}[1m])",
          "legendFormat": "{{service}} - {{instance}} - Upload",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        },
        {
          "expr": "avg_over_time(nextcloud_test_speed_mbytes_per_sec{service=~\"$service\",instance=~\"$instance\",type=\"download\"}[1m])",
          "legendFormat": "{{service}} - {{instance}} - Download",
          "refId": "B",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "MBs",
          "color": { "mode": "palette-classic" }
        } 
      },
      "gridPos": { "x": 0, "y": 24, "w": 24, "h": 8 },
      "id": 17,
      "options": {
        "tooltip": { "mode": "multi", "sort": "none" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "min"] }
      }
    },
    {
      "type": "heatmap",
      "title": "Upload Duration Distribution (Heatmap)",
      "targets": [
        {
          "expr": "increase(nextcloud_test_duration_seconds_bucket{service=~\"$service\",instance=~\"$instance\",type=\"upload\"}[5m])",
          "legendFormat": "{{le}}",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": { 
        "defaults": { 
          "unit": "s"
        } 
      },
      "gridPos": { "x": 0, "y": 32, "w": 12, "h": 8 },
      "id": 18
    },
    {
      "type": "table",
      "title": "Instance Performance Summary",
      "targets": [
        {
          "expr": "avg_over_time(nextcloud_test_duration_seconds{service=~\"$service\",instance=~\"$instance\"}[1h])",
          "legendFormat": "",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "format": "table",
          "instant": true
        },
        {
          "expr": "avg_over_time(nextcloud_test_speed_mbytes_per_sec{service=~\"$service\",instance=~\"$instance\"}[1h])",
          "legendFormat": "",
          "refId": "B",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "format": "table",
          "instant": true
        },
        {
          "expr": "avg_over_time(nextcloud_test_success{service=~\"$service\",instance=~\"$instance\"}[1h]) * 100",
          "legendFormat": "",
          "refId": "C",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "format": "table",
          "instant": true
        }
      ],
      "fieldConfig": { 
        "defaults": {
          "custom": {
            "displayMode": "auto",
            "cellOptions": {
              "type": "auto"
            }
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Value #A" },
            "properties": [
              { "id": "displayName", "value": "Avg Duration (s)" },
              { "id": "unit", "value": "s" }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Value #B" },
            "properties": [
              { "id": "displayName", "value": "Avg Speed (MB/s)" },
              { "id": "unit", "value": "MBs" }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Value #C" },
            "properties": [
              { "id": "displayName", "value": "Success Rate %" },
              { "id": "unit", "value": "percent" }
            ]
          }
        ]
      },
      "gridPos": { "x": 12, "y": 32, "w": 12, "h": 8 },
      "id": 19
    }
  ],
  "templating": {
    "list": [
      {
        "name": "service",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(nextcloud_test_duration_seconds,service)",
        "refresh": 1,
        "includeAll": true,
        "multi": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "instance",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(nextcloud_test_duration_seconds{service=~\"$service\"},instance)",
        "refresh": 1,
        "includeAll": true,
        "multi": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "annotations": { "list": [] }
}
